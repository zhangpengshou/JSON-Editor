{
  "manifest_version": 2,
  "name": "JSON Editor",
  "version": "1.0.2",
  "description": "JSON Editor.",
  "homepage_url": "https://github.com/zhangpengshou/JSON-Editor",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icons/32.png",
    "default_title": "JSON Editor"
  },
  "applications": {
    "gecko": {
      "id": "zhangpengshou@hotmail.com",
      "strict_min_version": "48.0"
    }
  },
  "content_security_policy": "script-src 'self' blob:; object-src 'self';"
}
